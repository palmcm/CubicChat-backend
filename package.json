{
  "name": "CubicChat-backend",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/palmcm/CubicChat-backend.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "lint": "yarn eslint . --ext .ts"
  },
  "compilerOptions": {
    "target": "es2016",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@types/express": "^4.17.17",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.12.0",
    "prisma-dbml-generator": "^0.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
