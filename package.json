{
  "name": "CubicChat-backend",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/palmcm/CubicChat-backend.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "dependencies": {
    "@prisma/client": "4.11.0",
    "express": "^4.18.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "prisma": "^4.11.0",
    "prisma-dbml-generator": "^0.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
